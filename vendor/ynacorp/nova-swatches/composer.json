{
    "name": "ynacorp/nova-swatches",
    "description": "A beautiful color picking field for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Yna\\NovaSwatches\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yna\\NovaSwatches\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}