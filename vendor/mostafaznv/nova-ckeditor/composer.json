{
    "name": "mostafaznv/nova-ckeditor",
    "description": "CkEditor for Laravel Nova",
    "keywords": [
        "laravel",
        "ckeditor",
        "ckeditor 5",
        "media picker",
        "image picker",
        "video picker",
        "snippets",
        "rich text editor",
        "WYSIWYG Editor",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "intervention/image": "^2.5",
        "mostafaznv/nova-video": "^1.0",
        "laravel/framework": "^8.40.0",
        "spatie/laravel-image-optimizer": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Mostafaznv\\NovaCkEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mostafaznv\\NovaCkEditor\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
